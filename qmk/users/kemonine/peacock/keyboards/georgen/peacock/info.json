{
    "manufacturer": "George Norton",
    "keyboard_name": "georgen/peacock",
    "maintainer": "george-norton",
    "processor": "RP2040",
    "bootloader": "rp2040",
    "encoder": {
        "enabled": true,
        "rotary": [
            {"pin_a": "GP20", "pin_b": "GP19"},
            {"pin_a": "GP18", "pin_b": "GP17"}
        ]
    },
    "features": {
        "bootmagic": true,
        "command": false,
        "console": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": false,
        "rgb_matrix": true,
        "digitizer": true,
        "pointing_device": true
    },
    "url": "https://github.com/george-norton/peacock",
    "usb": {
        "device_version": "1.0.0",
        "pid": "0x0000",
        "vid": "0xFEED"
    },
    "ws2812": {
        "pin": "GP25",
        "driver": "vendor"
    },
    "rgblight": {
        "max_brightness": 128,
        "animations": {
            "static_light": true
        },
        "default": {
            "animation": "static_light"
        }
    },
    "rgb_matrix": {
        "driver": "ws2812",
        "led_count": 6,
        "max_brightness": 128,
        "animations": {
            "solid_color": true
        },
        "default": {
            "animation": "solid_color"
        }
    }
}
